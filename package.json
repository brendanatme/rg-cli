{
  "name": "rg-cli",
  "version": "3.0.0",
  "description": "A React generator CLI tool for generating React components in Javascript or Typescript",
  "main": "./index.js",
  "bin": {
    "rg": "./index.js"
  },
  "scripts": {
    "gen": "node ./index.js g c",
    "gen:js": "node ./index.js g c --type=js --dir=\"./examples/js\"",
    "gen:ts": "node ./index.js g c --type=ts --dir=\"./examples/ts\"",
    "gen:css": "node ./index.js g c --type=ts --style=css --dir=\"./examples/css\"",
    "gen:scss": "node ./index.js g c --type=ts --style=scss --dir=\"./examples/scss\"",
    "release": "node ./scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brendanatme/rg-cli.git"
  },
  "author": "brendanatme",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brendanatme/rg-cli/issues"
  },
  "homepage": "https://github.com/brendanatme/rg-cli#readme",
  "dependencies": {
    "minimist": "^1.2.0"
  },
  "keywords": [
    "react",
    "typescript",
    "component",
    "generator",
    "cli"
  ]
}
