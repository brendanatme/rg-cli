{
  "name": "rg-cli",
  "version": "1.2.0",
  "description": "A React generator CLI tool for generating React components in Typescript",
  "main": "./index.js",
  "bin": {
    "rg": "./index.js"
  },
  "scripts": {
    "gen": "node ./index.js g c",
    "example:js": "node ./index.js g c --dir=\"./examples/js\"",
    "example:ts": "node ./index.js g c --ts --dir=\"./examples/ts\"",
    "release": "node ./scripts/release.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brendanatme/rg-cli.git"
  },
  "author": "brendanatme",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brendanatme/rg-cli/issues"
  },
  "homepage": "https://github.com/brendanatme/rg-cli#readme",
  "dependencies": {
    "minimist": "^1.2.0"
  },
  "keywords": [
    "react",
    "typescript",
    "component",
    "generator",
    "cli"
  ]
}
